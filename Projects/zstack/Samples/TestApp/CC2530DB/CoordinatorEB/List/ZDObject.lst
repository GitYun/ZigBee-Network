    415           * @param       state - The current device state.
    416           *
    417           * output parameters
    418           *
    419           * None.
    420           *
    421           * @return      None.
    422           **************************************************************************************************
    423           */
    424          static void zdoSendStateChangeMsg(uint8 state, uint8 taskId)
    425          {
    426            osal_event_hdr_t *pMsg = (osal_event_hdr_t *)osal_msg_find(taskId, ZDO_STATE_CHANGE);
    427          
    428            if (NULL == pMsg)
    429            {
    430              if (NULL == (pMsg = (osal_event_hdr_t *)osal_msg_allocate(sizeof(osal_event_hdr_t))))
    431              {
    432                // Upon failure to notify any EndPoint of the state change, re-set the ZDO event to
    433                // try again later when more Heap may be available.
    434                osal_set_event(ZDAppTaskID, ZDO_STATE_CHANGE_EVT);
    435              }
    436              else
    437              {
    438                pMsg->event = ZDO_STATE_CHANGE;
    439                pMsg->status = state;
    440          
    441                (void)osal_msg_send(taskId, (uint8 *)pMsg);
    442              }
    443            }
    444            else
    445            {
    446              // Modify in place the status of an existing ZDO_STATE_CHANGE message to the EndPoint.
    447              pMsg->status = state;
    448            }
    449          }
