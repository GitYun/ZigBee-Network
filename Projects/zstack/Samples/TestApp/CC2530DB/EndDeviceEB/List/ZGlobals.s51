//  403 
//  404 static uint8 zgItemInit( uint16 id, uint16 len, void *buf, uint8 setDefault );
//  405 
//  406 
//  407 /*********************************************************************
//  408  * @fn       zgItemInit()
//  409  *
//  410  * @brief
//  411  *
//  412  *   Initialize a global item. If the item doesn't exist in NV memory,
//  413  *   write the system default (value passed in) into NV memory. But if
//  414  *   it exists, set the item to the value stored in NV memory.
//  415  *
//  416  *   Also, if setDefault is TRUE and the item exists, we will write
//  417  *   the default value to NV space.
//  418  *
//  419  * @param   id - item id
//  420  * @param   len - item len
//  421  * @param   buf - pointer to the item
//  422  * @param   setDefault - TRUE to set default, not read
//  423  *
//  424  * @return  ZSUCCESS if successful, NV_ITEM_UNINIT if item did not
//  425  *          exist in NV, NV_OPER_FAILED if failure.
//  426  */
//  427 static uint8 zgItemInit( uint16 id, uint16 len, void *buf, uint8 setDefault )
//  428 {
//  429 
//  430   uint8 status;
//  431 
//  432   // If the item doesn't exist in NV memory, create and initialize
//  433   // it with the value passed in.
//  434   status = osal_nv_item_init( id, len, buf );
//  435   if ( status == ZSUCCESS )
